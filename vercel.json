{
  "version": 2,
  "buildCommand": "npx prisma generate && npm run build --legacy-peer-deps",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": "nextjs",
  "env": {
    "PRISMA_GENERATE_DATAPROXY": "true"
  },
  "routes": [
    {
      "src": "/login",
      "dest": "/login"
    },
    {
      "src": "/register",
      "dest": "/register"
    },
    {
      "src": "/dashboard",
      "dest": "/dashboard"
    },
    {
      "src": "/admin",
      "dest": "/admin"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
